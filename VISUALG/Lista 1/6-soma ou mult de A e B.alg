algoritmo "semnome"
var
a,b,c: inteiro
inicio
escreval("Digite o valor de A :")
leia(a)
escreval("Digite o valor de B :")
leia(b)
se( a = b ) entao
   c <- a+b
   escreval("A soma dos valores é : ")
   escreva(c)
senao
     c <- a*b
   escreval("A multiplicação dos valores é : ")
   escreva(c)
fimse




fimalgoritmo
